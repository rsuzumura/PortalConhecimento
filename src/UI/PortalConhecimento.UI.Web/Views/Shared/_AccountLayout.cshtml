<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/toastr")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container">
        <div class="row header">
            <div class="col-xs-6 marca"></div>
        </div>
        <div class="row bottom-space">
            <ul id="menu" class="anonymous-menu center-block">
                <li><a href="#" class="corporate"></a></li>
                <li><a href="#" class="aulas"></a></li>
                <li><a href="@Url.Action("Index", "FAQ")" class="duvidas"></a></li>
                <li><a href="@Url.Action("Index", "FaleConosco")" class="fale-conosco"></a></li>
            </ul>
        </div>
        <div class="row top-banner"></div>
        @RenderBody()
        <div class="row rodape">
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/toastr")
    @RenderSection("Scripts", false)
</body>
</html>